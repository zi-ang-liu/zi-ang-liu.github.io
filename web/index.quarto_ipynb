   "source": [
    "from ipyleaflet import Map, basemaps, basemap_to_tiles\n",
    "\n",
    "lat = 48\n",
    "long = 350\n",
    "\n",
    "print(\"title=\", f\"World Map at {lat}, {long}\")\n",
    "\n",
    "Map(basemap=basemap_to_tiles(basemaps.OpenStreetMap.Mapnik),\n",
    "    center=(lat, long), zoom=2)"
   ]
  }
